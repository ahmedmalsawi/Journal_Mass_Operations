<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Mass Set to Draft Action -->
    <record id="mass_move_to_draft_action" model="ir.actions.server">
        <field name="name">Set to Draft</field>
        <field name="model_id" ref="account.model_account_move"/>
        <field name="binding_model_id" ref="account.model_account_move"/>
        <field name="binding_view_types">list</field>
        <field name="state">code</field>
        <field name="code">
action = env['account.move'].action_mass_move_to_draft()
        </field>
    </record>

    <!-- Mass Post Action -->
    <record id="mass_move_to_post_action" model="ir.actions.server">
        <field name="name">Post Selected</field>
        <field name="model_id" ref="account.model_account_move"/>
        <field name="binding_model_id" ref="account.model_account_move"/>
        <field name="binding_view_types">list</field>
        <field name="state">code</field>
        <field name="code">
action = env['account.move'].action_mass_move_to_post()
        </field>
    </record>

    <!-- Mass Cancel Action -->
    <record id="mass_move_cancel_action" model="ir.actions.server">
        <field name="name">Cancel Selected</field>
        <field name="model_id" ref="account.model_account_move"/>
        <field name="binding_model_id" ref="account.model_account_move"/>
        <field name="binding_view_types">list</field>
        <field name="state">code</field>
        <field name="code">
action = env['account.move'].action_mass_move_cancel()
        </field>
    </record>

    <!-- Mass Print Action -->
    <record id="mass_print_entries_action" model="ir.actions.server">
        <field name="name">Print Selected</field>
        <field name="model_id" ref="account.model_account_move"/>
        <field name="binding_model_id" ref="account.model_account_move"/>
        <field name="binding_view_types">list</field>
        <field name="state">code</field>
        <field name="code">
action = env['account.move'].action_mass_print_entries()
        </field>
    </record>
</odoo>